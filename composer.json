{
    "name": "transprime-research/laravel-migrations-snapshot",
    "type": "project",
    "description": "The Migrations Snapshot for Laravel.",
    "keywords": [
        "database",
        "migration",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.2",
        "doctrine/dbal": "^2.10",
        "nette/php-generator": "^3.3",
        "transprime-research/piper": "^2.0",
        "laravel/framework": ">=5.7"
    },
    "require-dev": {
        "phpunit/phpunit": ">=8.0"
    },
    "autoload": {
        "psr-4": {
            "Transprime\\MigrationsSnapshot\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Transprime\\MigrationsSnapshot\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Transprime\\MigrationSnapshot\\Providers\\MigrationsSnapshotServiceProvider"
            ]
        }
    }
}

{
    "name": "transprime/laravel-migration-snapshot",
    "type": "project",
    "description": "The Migration Snapshot for Laravel.",
    "keywords": [
        "database",
        "migration",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.2",
        "doctrine/dbal": "^2.10",
        "nette/php-generator": "^3.3",
        "transprime-research/piper": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": ">=8.0"
    },
    "autoload": {
        "psr-4": {
            "Transprime\\MigrationSnapshot": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Transprime\\MigrationSnapshot\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
